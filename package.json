{
  "name": "turbo-shad-storybook-starter",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:web": "turbo run dev --filter ui --filter web",
    "dev:docs": "turbo run dev --filter ui --filter docs",
    "dev:both": "turbo run dev --filter ui --filter doca --filter web",
    "dev:ui": "turbo run dev --filter ui --filter storybook",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "ui:add": "pnpm --filter ui ui:add"
  },
  "devDependencies": {
    "@turbo/gen": "^1.9.7",
    "eslint": "^7.32.0",
    "eslint-config-custom": "workspace:*",
    "prettier": "^2.5.1",
    "turbo": "latest"
  },
  "packageManager": "pnpm@7.15.0"
}
